{
  "name": "@runanywhere/rag",
  "version": "0.1.0",
  "description": "RAG (Retrieval-Augmented Generation) backend for RunAnywhere React Native SDK - On-device document search with LLM",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "source": "src/index",
  "files": [
    "src",
    "cpp",
    "ios",
    "android",
    "nitrogen",
    "nitro.json",
    "react-native.config.js",
    "*.podspec"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "nitrogen": "nitrogen",
    "build": "tsc",
    "prepare": "nitrogen && npm run build"
  },
  "keywords": [
    "react-native",
    "runanywhere",
    "rag",
    "retrieval",
    "augmented",
    "generation",
    "vector-search",
    "embedding",
    "on-device",
    "nitro",
    "expo"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@runanywhere/core": ">=0.16.0",
    "@runanywhere/llamacpp": ">=0.17.0",
    "@runanywhere/onnx": ">=0.17.0",
    "react": ">=18.0.0",
    "react-native": ">=0.74.0",
    "react-native-nitro-modules": ">=0.31.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "react": "^18.2.0",
    "react-native": "^0.74.0",
    "react-native-nitro-modules": "^0.31.3",
    "typescript": "^5.3.3"
  },
  "homepage": "https://github.com/RunanywhereAI/runanywhere-sdks#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/RunanywhereAI/runanywhere-sdks.git",
    "directory": "sdk/runanywhere-react-native/packages/rag"
  },
  "bugs": {
    "url": "https://github.com/RunanywhereAI/runanywhere-sdks/issues"
  }
}
